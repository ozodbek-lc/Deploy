server {
    listen 80;
    server_name 164.90.175.195;

    location = /favicon.ico { access_log off; log_not_found off; }

    location /static/ {
        root /var/www/furni;
    }

    location /media/ {
        root /var/www/furni;
    }

    location / {
        include proxy_params;
        proxy_pass http://unix:/run/gunicorn.sock;
    }
}